  {
    "name": "MDL Chrome Plugin",
    "version": "1.0",
    "description": "Build an Extension for the hack!",
    "manifest_version": 2,
    "permissions": ["storage"],
     "chrome_url_overrides": {
		"newtab": "mdlTab.html"
	  },
    "browser_action": {
	   "default_icon": "icon.png",
	   "default_popup": "popup.html",
		"default_title": "Click here!"
	  },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }